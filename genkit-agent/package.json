{
  "name": "genkit-agent",
  "version": "1.0.0",
  "description": "Bob's Brain - Vertex AI Agent powered by Genkit 2",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "genkit start -- tsx --watch src/index.ts",
    "start": "node dist/index.js",
    "deploy": "gcloud functions deploy bob-agent --gen2 --runtime=nodejs20 --region=us-central1 --source=. --entry-point=bobAgent --trigger-http --allow-unauthenticated"
  },
  "keywords": ["genkit", "vertex-ai", "slack", "ai-agent"],
  "author": "Jeremy Longshore",
  "license": "MIT",
  "type": "commonjs",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@genkit-ai/core": "^1.22.0",
    "@genkit-ai/googleai": "^1.22.0",
    "@genkit-ai/vertexai": "^1.22.0",
    "@types/node": "^24.10.0",
    "firebase-admin": "^13.6.0",
    "typescript": "^5.9.3"
  }
}
