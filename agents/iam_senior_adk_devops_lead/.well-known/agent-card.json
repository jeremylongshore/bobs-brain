              "type": "object",
              "properties": {
                "step_number": {
                  "type": "integer"
                },
                "worker": {
                  "type": "string"
                },
                "task": {
                  "type": "string"
                },
                "dependencies": {
                  "type": "array",
                  "items": {
                    "type": "string"
                  }
                }
              }
            }
          },
          "execution_order": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "description": "Ordered list of workers to execute"
          },
