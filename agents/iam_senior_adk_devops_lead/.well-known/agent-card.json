  "skills": [
    {
      "name": "Route Task to Worker",
      "description": "Analyze task requirements and route to appropriate IAM specialist worker",
      "input_schema": {
        "type": "object",
        "required": [
          "task_description"
        ],
        "properties": {
          "task_description": {
            "type": "string",
            "description": "Description of the software engineering task"
          },
          "task_type": {
            "type": "string",
            "enum": [
              "adk_design",
              "issue_creation",
              "fix_planning",
              "fix_implementation",
              "qa_validation",
              "documentation",
              "cleanup",
              "indexing"
            ],
            "description": "Type of task (if known)"
          },
          "priority": {
            "type": "string",
            "enum": [
              "high",
              "medium",
              "low"
            ],
            "default": "medium",
            "description": "Task priority level"
          }
        }
      },
      "output_schema": {
        "type": "object",
        "required": [
          "assigned_worker",
          "routing_rationale"
        ],
        "properties": {
          "assigned_worker": {
            "type": "string",
            "description": "Worker agent assigned to task (e.g., iam-adk, iam-issue)"
          },
          "routing_rationale": {
            "type": "string",
            "description": "Explanation of why this worker was selected"
          },
          "task_breakdown": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "description": "Breakdown of task into subtasks (if applicable)"
          }
        }
      },
      "id": "foreman.route_task",
      "tags": [
        "foreman",
        "route_task"
      ],
      "examples": []
    },
    {
      "name": "Coordinate Multi-Agent Workflow",
      "description": "Orchestrate a multi-step workflow across multiple IAM worker agents",
      "input_schema": {
        "type": "object",
        "required": [
          "workflow_description"
        ],
        "properties": {
          "workflow_description": {
            "type": "string",
            "description": "Description of the overall workflow"
          },
          "workflow_steps": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "step_name": {
                  "type": "string"
                },
                "worker_agent": {
                  "type": "string"
                },
                "dependencies": {
                  "type": "array",
                  "items": {
                    "type": "string"
                  }
                }
              }
            },
            "description": "Optional explicit workflow steps"
          }
